{
  "dir": "/path/to/cache",
  "log_level": "info",
  "remote": "s3",
  "s3": {
    "region": "us-east-1",
    "bucket": "my-bucket",
    "access_key": "your-access-key",
    "secret_access_key": "your-secret-key",
    "endpoint": "https://s3.amazonaws.com",
    "disable_ssl": false,
    "use_path_style": false
  },
  "github": {
    "token": "your-github-token"
  }
}
