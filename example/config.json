{
  "dir": "/path/to/cache",
  "log_level": "info",
  "remote": "none",
  "s3": {
    "region": "us-east-1",
    "bucket": "my-bucket",
    "access_key": "your-access-key",
    "secret_access_key": "your-secret-key",
    "endpoint": "s3.amazonaws.com",
    "disable_ssl": false,
    "use_path_style": false
  },
  "github": {
    "cache_url": "https://actions.example.com/cache",
    "token": "your-github-token",
    "runner_os": "Linux",
    "ref": "refs/heads/main",
    "sha": "0123456789abcdef0123456789abcdef01234567"
  }
}
